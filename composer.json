{
    "name": "timeless/module-wi-receive",
    "description": "TMS WAI receive package",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Sam Bramley",
            "email": "sam@cloudstacks.co"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.7",
        "orchestra/testbench-browser-kit": "^3.7",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WAI\\Receive\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WAI\\Receive\\Tests\\": "tests"
        }
    },
    "suggest": {
        "sensiolabs/security-checker": "Required to use the security check command"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"mkdir('src'.DIRECTORY_SEPARATOR.'Http');\"",
            "php -r \"mkdir('src'.DIRECTORY_SEPARATOR.'Http'.DIRECTORY_SEPARATOR.'Controllers');\"",
            "php -r \"mkdir('src'.DIRECTORY_SEPARATOR.'Http'.DIRECTORY_SEPARATOR.'Middleware');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "WAI\\Receive\\ReceiveServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
